import java.util.Scanner;

public class Aufgabe4 {

	public static void main(String[] args) {
		Scanner obj = new Scanner(System.in);
		if(obj.hasNextInt()) {
			int amoutOfInputs = obj.nextInt();
			if(amoutOfInputs>0) {
				obj.nextLine();
				System.out.println("Amount: "+amoutOfInputs);
			}
		} else {
			System.out.println("Wrong Datatype");
		}
		
		obj.close();
		
		
	/*	for(int i=0;i<amoutOfInputs;i++) {
			Scanner obj = new Scanner(System.in);
			int amoutOfInputs = obj.nextInt();
		}*/
	}

}
