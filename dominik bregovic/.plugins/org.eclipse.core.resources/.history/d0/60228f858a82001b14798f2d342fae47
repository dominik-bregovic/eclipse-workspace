			}else {
				System.out.print(","+numbers[i]);
			} 
			String convert = Character.toString(numbers[i]);
			sumOfDigits += Long.parseLong(convert);
		}
		
		System.out.println("]");
		System.out.println("Sum of all digits: "+sumOfDigits);
		
