import java.util.Scanner;

public class Aufgabe4 {

	public static void main(String[] args) {
		int amountOfInputs=3;
		Scanner obj = new Scanner(System.in);
		if(obj.hasNextInt()) {					//checking if int
			int amoutOfInputs = obj.nextInt();					//saving int in amoutOfInputs
			if(amoutOfInputs>0) {								//checking if positive int
				System.out.println("Amount: "+amoutOfInputs);
			}
		}else {
			System.out.println("Wrong Datatype");
		}
		
		obj.close();
		
		
	/*	for(int i=0;i<amoutOfInputs;i++) {
			Scanner obj = new Scanner(System.in);
			int amoutOfInputs = obj.nextInt();
		}*/
	}

}
